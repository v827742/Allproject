package com.cjc;

public class A {
	public static void main(String[] args) {
		
	
	System.out.println("A");
	
	
	for(int i=1;i<=5;i++)
	{
		for(int j=1;j<=5;j++)
		{
			System.out.print(i);
		}
		System.out.println();
	     }
	System.out.println("<<<<<<<<<<<<<<>>>>>>>>>>>>>>");
	{
		
		
		System.out.println("B");
	for(int k=5;k>=1;k--)              
	{
		for(int l=5;l>=1;l--)
		{
			System.out.print(k);
		}
		System.out.println();
	}}
	System.out.println("<<<<<<<<<<<<><<<<<<<<><<<<<<<<<>>>>>");
	{

		System.out.println("C");
    
		
		for(int o=1;o<=5;o++)
     {
    	 for(int p=1;p<=5;p++)
    	 {
    		 System.out.print(p);
    	 }
    		 System.out.println();
     }}
  System.out.println("<<<<<<<<<<<<<<<<<<<<<<<<<>>>>>>>>>>>>>>>>>>>>>");
    
  
  System.out.println("D");
  
  
  for(int q=5;q>=1;q--)
  {
	  for(int s=5;s>=1;s--) {
		  System.out.print(s);
	  }
	  System.out.println();
  }
  System.out.println("<<<<<<<<<<<<<<<<<<>>>>>>>>>>>>>>>>>>");
	
  System.out.println("E");
  
  
  for(int m=1;m<=5;m++) {
	  for(int n=5;n>=m;n--)
	  {
		  System.out.print(m);
		  
	  }
	  System.out.println();
  }
  System.out.println("<<<<<<<<<<<<<<<<>>>>>>>>>>>>");
  
  System.out.println("F");
  
  
  for(int a=5;a>=1;a--)
  {
	  for(int b=1;b<=a;b++)
	  {
		  System.out.print(a);
	  }
	  System.out.println();
  }
  System.out.println("<<<<<<<<<<<>>>>>>>>>>><<<<>>>>>>>>>>>");
	
  System.out.println("G");
	
  
  for(int g=5;g>=1;g--)
	{
		for(int f=1;f<=g;f++)
		{
			System.out.print(f);
		}
		System.out.println();
	}
	System.out.println("<<<<<<<<<<<<<<<<>>>>>>>>>>>>>>>>>>>");
	
	System.out.println("H");
	for(int t=1;t<=5;t++)
	{
		for(int r=1;r<=t;r--)
		{
			System.out.print("*");
		}
		System.out.println();
	}
	
	}
}


